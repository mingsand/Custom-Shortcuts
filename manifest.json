{
  "manifest_version": 3,
  "name": "Custom Shortcuts",
  "version": "1.0",
  "description": "Open specific URLs with custom keyboard shortcuts.",
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "open-youtube": {
      "suggested_key": {
        "default": "Alt+Y"
      },
      "description": "Open YouTube"
    },
    "open-github": {
      "suggested_key": {
        "default": "Alt+G"
      },
      "description": "Open GitHub"
    }
  }
}
